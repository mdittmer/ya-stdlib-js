{
  "plugins": [
    "esformatter-add-trailing-commas",
    "esformatter-auto-wrap",
    "esformatter-onevar",
    "esformatter-semicolons",
    "esformatter-var-each"
  ],
  "autoWrap": {
    "maxLineLength": 80
  },
  "indent": {
    "value": "  ",
    "arguments": 0
  },
  "lineBreak": {
    "after": {
      "VariableDeclaration": "<=2"
    }
  }
}
